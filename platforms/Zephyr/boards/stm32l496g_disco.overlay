	sampling-times = < 0x3 0x7 0xd 0x19 0x30 0x5d 0xf8 0x281 >;
	st,adc-sequencer = "FULLY_CONFIGURABLE";
	st,adc-oversampler = "OVERSAMPLER_MINIMAL";
