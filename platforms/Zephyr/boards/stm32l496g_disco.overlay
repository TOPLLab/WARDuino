&pinctrl {
	adc3_test: adc3_test {
		pinmux = <STM32_PINMUX('F', 0, ANALOG)>;
	};
};

&adc3 {
	status = "okay";
	st,adc-clock-source = "SYNC";
	st,adc-prescaler = < 0x4 >;
